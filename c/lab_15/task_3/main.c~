#include <stdio.h>

char *get_month_name(int n) {
  static char *names[12] = {
    "Illegal month",
    "January", "February", "March",
    "April", "May", "June",
    "July", "August", "September",
    "October", "November", "December"
  };

  return ((n >= 1) && (n <= 12)) ? (names[n]) : (names[0]);
}

int main(int argc, char **argv) {
  return 0;
}

